{
  "name": "@fro-bot/agent",
  "version": "0.10.2",
  "private": true,
  "description": "@fro-bot Agent",
  "keywords": [
    "github-actions",
    "nodejs",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fro-bot/agent.git"
  },
  "license": "MIT",
  "author": "Fro Bot <agent@fro.bot>",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "bootstrap": "pnpm install --no-strict-peer-dependencies --prefer-offline --loglevel warn",
    "build": "pnpm check-types && tsdown",
    "check-types": "tsc --noEmit",
    "fix": "eslint --fix",
    "postinstall": "simple-git-hooks",
    "lint": "eslint",
    "test": "vitest run"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,jsx,tsx,css,md,json,yaml,yml}": [
      "pnpm run fix"
    ]
  },
  "prettier": "@bfra.me/prettier-config/120-proof",
  "dependencies": {
    "@actions/cache": "^5.0.1",
    "@actions/core": "2.0.1",
    "@actions/exec": "^2.0.0",
    "@actions/github": "^7.0.0",
    "@actions/tool-cache": "^2.0.2",
    "@bfra.me/es": "0.1.0",
    "@octokit/auth-app": "8.1.2",
    "@opencode-ai/sdk": "1.1.11"
  },
  "devDependencies": {
    "@bfra.me/eslint-config": "0.47.6",
    "@bfra.me/prettier-config": "0.16.6",
    "@bfra.me/tsconfig": "0.12.1",
    "@semantic-release/git": "10.0.1",
    "@types/node": "24.10.4",
    "@vitest/eslint-plugin": "1.6.4",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "generate-license-file": "4.1.1",
    "jiti": "2.6.1",
    "js-yaml": "4.1.1",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "read-package-up": "12.0.0",
    "semantic-release": "25.0.2",
    "simple-git-hooks": "2.13.1",
    "tsdown": "0.18.4",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "packageManager": "pnpm@10.27.0"
}
